{
    "content_scripts": [
        {
            "js": [
                "js/jquery.min.js",
                "js/moment.min.js",
                "js/read.js"
            ],
            "matches": [
                "https://banweb.cityu.edu.hk/*"
            ],
            "run_at": "document_end",
            "all_frames": true
        }
    ],
    "background": {
        "service_worker": "js/bg.js"
    },
    "action": {
        "default_icon": {
            "24": "img/baseline_schedule_white_24dp.png"
        },
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "img/icon.png"
    },
    "description": "Timetable checker for CityU mates.",
    "manifest_version": 3,
    "name": "CityU Schedule Planner",
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "https://banweb.cityu.edu.hk/*"
    ],
    "version": "0.1"
}